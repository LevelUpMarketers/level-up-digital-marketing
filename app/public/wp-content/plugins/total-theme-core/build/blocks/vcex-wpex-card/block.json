    "style": {
      "type": "string"
    },
    "link_type": {
      "type": "string"
    },
    "title_tag": {
      "type": "string"
    },
    "thumbnail_size": {
      "type": "string"
    },
    "postId": {
      "type": "string"
    },
    "excerpt_length": {
      "type": "string"
    },
    "more_link_text": {
      "type": "string"
    },
    "title_font_size": {
      "type": "string"
    },
